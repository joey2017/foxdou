    <frame name="right-frame" noresize="noresize" marginwidth="0" marginheight="0" frameborder="0" scrolling="auto" marginheight="0"
           src="{{ path(right_route_path, {'id': id}) }}">
    <!-- /right-->
</frameset>
</html>