     * 服务详情Tab标签的切换
     */
    layui.use(['form','element'], function(){
        var form = layui.form();
        var element = layui.element(); //Tab的切换功能，切换事件监听等，需要依赖element模块
        $('.details-api-tab').find('a:first').click();
    });


    layui.use('layer', function(){ //独立版的layer无需执行这一句
        var $ = layui.jquery, layer = layui.layer; //独立版的layer无需执行这一句

        //触发事件
        var active = {
            xiala: function(){
                //充值app secret层
                layer.open({
                    type: 1
                    ,title: '请选择'
                    ,closeBtn: true
                    ,area: '300px'
                    ,shade: 0
                    ,id: 'LAY_layuipro' //设定一个id，防止重复弹出
                    ,moveType: 1 //拖拽模式，0或者1
                    ,content: $('.info-2').show()
                    ,success: function(layero){
                        $('#queding').click(function(){
                            layer.closeAll();
                        });
                    }
                });
            }
        };
        $('.site-demo-layer').on('click', function(){
            var type = $(this).data('type');
            active[type] ? active[type].call(this) : '';
        });
    });



    /*
     * 服务详情-API小分类切换
     */
    $('.details-api-tab a').on('click',function(){
        var key=$(this).attr('key');
        $(this).addClass('details-api-tab-this');
        $(this).siblings().removeClass('details-api-tab-this');
        $('.details-api-content[key='+key+']').show();
        $('.details-api-content').not('[key='+key+']').hide();

    });


    /*
     * 导航菜单鼠标经过样式
     */
    $('#menu li').click(function(){
        $(this).addClass('menu-this');
        $(this).siblings().removeClass('menu-this');
    });


    /*
     * 导航背景变色
     */
    $('.top').hover(
        function(){
            $(this).css('background','rgba(255,255,255,.1)');
        },
        function(){
            $(this).css('background','none');
        }
    );

    /*
     * 导航下拉展示
     */
    $(".menu-list").find('.menu-item').first().addClass('menu-item-hover');
    $(".menu-event").hover(
        function () {
            $(".menu-list").slideDown("fast");
        },
        function () {
            $(".menu-list").stop(true).slideUp('fast');
        }
    );
    $('.menu-title').mouseenter(function(){
        $(this).parent().addClass('menu-item-hover').siblings().removeClass('menu-item-hover');
        $(this).parent().find('.menu-body').hide();
    });


    /*
     * 加减按钮的递增递减
     */
    function jia_jie(type,dom){
        var num = Number($(dom).val());
        if(type=='+'){
            num = num + 1;
        }else if(type=='-'){
            if (num > 1) {
                num = num - 1;
            }
        }
        $(dom).val(num);
    }
    $('.add-input').click(function(){
        jia_jie('+','.num-input');
    });
    $('.reduce-input').click(function(){
        jia_jie('-','.num-input');
    });


    /*
     *收藏图标添加点击样式
     */
    function add_class_shoucang(dom){
        var aa=$(dom).find('i').is('li-this');
        $(dom).find('i').addClass('li-this');
    }


    /*
     * 扣费方式模块切换
     */
    function koufeifangshi(this_dom){
        var key = $(this_dom).attr('key');
        $(this_dom).addClass('promo-type-this');
        $(this_dom).siblings().removeClass('promo-type-this');
        $('.detail-arrt').find('div[key='+key+']').show();
        $('.detail-arrt').find('div').not('div[key='+key+']').hide();
    }


    /*
     * 点击“立即申请”按钮，弹出登录框
     */
    function lijishenqing(){
        $('.blackMask').show();
        $('body').css({overflow:'hidden'});    //禁用滚动条
    }


    /*
     * 点击登录框又上角的 叉叉，关闭登录框
     */
    function dengluClose(){
        $('.blackMask').hide();
        $('body').css({overflow:'auto'});
    }


    /*
     * 说明文字溢出隐藏，点击展开收缩
     */
    $('.shuoming').mouseover(function(){
        if (this.offsetWidth < this.scrollWidth) {
            $(this).attr('title','点击展开');
            $(this).css('cursor','pointer');
        }
    });
    function more_p(t){
        $(t).siblings('p').removeClass('open-p');
        $(t).remove();
    }
    function shuoming(this_dom){
        if (this_dom.offsetWidth < this_dom.scrollWidth) {
            $(this_dom).addClass('open-p');
            $(this_dom).after("<span class='more-p' onclick='more_p(this);'>收起</span>") ;
        }
    }
    $('.shuoming').click(function(){
        shuoming(this);
    });


</script>

